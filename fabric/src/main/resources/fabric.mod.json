{
  "schemaVersion": 1,
  "id": "megacells",
  "version": "$version",

  "name": "MEGA Cells",
  "description": "ME Greater Accumulation Cells - For when kilobytes just won't do.",
  "authors": ["90"],
  "contact": {
    "homepage": "https://mega.90.gripe",
    "sources": "https://github.com/62832/MEGACells"
  },

  "license": "LGPLv3.0",
  "icon": "icon.png",

  "environment": "*",
  "entrypoints": {
    "ae2": ["gripe._90.megacells.MEGACellsFabric"],
    "ae2:client": ["gripe._90.megacells.MEGACellsClient"],

    "fabric-datagen": ["gripe._90.megacells.datagen.MEGADataGenerators"]
  },
  "mixins": ["megacells.mixins.json"],

  "depends": {
    "minecraft": "$minecraftVersion",
    "ae2": ">=$ae2Version"
  },
  "suggests": {
    "ae2wtlib": ">=$ae2wtVersion",
    "appbot": ">=$appbotVersion"
  }
}
